<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite verbose="1" name="Carina Demo Tests - Wikipedia Multiple Browser Sample" parallel="tests" thread-count="2">

    <test name="homeFirefoxTest" >
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="com.qaprosoft.carina.demo.WebLocalizationAndInternalizationSample">
                <methods>
                    <include name="openHomeInDifferentLanguagesTest"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="homeChromeTest" >
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="com.qaprosoft.carina.demo.WebLocalizationAndInternalizationSample">
                <methods>
                    <include name="openHomeInDifferentLanguagesTest"/>
                </methods>
            </class>
        </classes>
    </test>

</suite>